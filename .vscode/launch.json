{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Vite",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "dev:debug"],
      "skipFiles": ["<node_internals>/**"],
      "console": "integratedTerminal"
    },
    {
        "name": "Debug Frontend",
        "type": "chrome",
        "request": "launch",
        "url": "http://localhost:3000/",
        "webRoot": "${workspaceFolder}/src"
    }
  ]
}
